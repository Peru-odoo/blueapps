<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_partner_form_inherit" model="ir.ui.view">
            <field name="name">consignado.view_partner.form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='contact_addresses']" position="before">
                    <page name="consignado" string="Consignado" autofocus="autofocus">
                            <group>
                                <field name="matricula_id"  nolabel="1">
                            </group>
                            <group>
                                <field name="contrato_id"  nolabel="1"/>
                            </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
